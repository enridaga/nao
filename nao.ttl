            rdfs:domain :NewsAngle ;
            rdfs:range :FabulaSpace .


###  http://purl.org/news/angles/hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:domain :Event .


###  http://purl.org/news/angles/hasMember
:hasMember rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain :Set ;
           rdfs:range owl:Thing .


###  http://purl.org/news/angles/hasNewsAngle
:hasNewsAngle rdf:type owl:ObjectProperty ;
              rdfs:domain :Fabula ;
              rdfs:range :NewsAngle .


###  http://purl.org/news/angles/hasRange
:hasRange rdf:type owl:ObjectProperty ;
          rdfs:domain :Function .


###  http://purl.org/news/angles/hasSource
:hasSource rdf:type owl:ObjectProperty ;
